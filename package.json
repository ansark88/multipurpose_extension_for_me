	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/chrome": "^0.0.306",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.2",
		"@wxt-dev/module-react": "^1.1.2",
		"typescript": "^5.6.3",
		"wxt": "^0.19.13"
	}
