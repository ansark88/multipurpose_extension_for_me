{
	"name": "wxt-react-starter",
	"description": "manifest.json description",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "wxt --debug",
		"dev:firefox": "wxt -b firefox --debug",
